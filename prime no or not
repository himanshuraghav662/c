
#include <stdio.h>
#include<conio.h>
#include<math.h>

int main() {
    int n;
    int i;
    int flag=0;
    printf("enter a no to check prime or not\n");
    scanf("%d",&n);
    for(i=2;i<=sqrt(n);i++)
    {
        if(n%i==0)
        {
            flag=1;
            break;
        }
    }
    if(flag==1)
    {
        printf("not a prime no");
    }
    else
    {
        printf("it is prime no");
    }

    return 0;
}
